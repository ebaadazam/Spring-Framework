package com.ebaad.SpringMvcModelAttrDemo.model;

public class Watch {
	private int price;
	private String model;
	
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public String getModel() {
		return model;
	}
	public void setModel(String model) {
		this.model = model;
	}
	
	@Override
	public String toString() {
		return "The Price of the Watch is " + price + ", and its Model is " + model;
	}
}

